{
  "name": "mapi-action",
  "version": "1.0.0",
  "private": true,
  "description": "Mayhem for API action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn run build && yarn run format && yarn run lint && yarn run package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForAllSecure/mapi-action.git"
  },
  "keywords": [
    "actions",
    "fuzzing",
    "api",
    "security"
  ],
  "author": "ForAllSecure",
  "dependencies": {
    "@actions/core": "^1.2.7",
    "@actions/exec": "^1.0.4",
    "@actions/tool-cache": "^1.6.1",
    "axios": "^0.21.1",
    "slugify": "^1.5.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.1",
    "@typescript-eslint/parser": "^4.25.0",
    "@vercel/ncc": "^0.28.5",
    "eslint": "^7.26.0",
    "eslint-plugin-github": "^4.1.3",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1"
  }
}
